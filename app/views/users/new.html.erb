<h1>New User Signup</h1>
<%= form_for :user do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :height %>
<%= f.text_field :height %>

<%= f.label :tickets %>
<%= f.text_field :tickets %>

<%= f.label :nausea %>
<%= f.text_field :nausea %>

<%= f.label :admin %>
<%= f.check_box :admin, {}, true %>

<%= f.label :happiness %>
<%= f.text_field :happiness %>

<%= f.label :password %>
<%= f.password_field :password %>

<%= f.submit "Create User" %>
<% end %>